<div class="col-md-4">
    <div class="card mb-4 player-card">
        <img src="{{photo}}" class="card-img-top" alt="{{name}} {{lastName}}">
        <div class="card-body">
            <h5 class="card-title">{{name}} {{lastName}}</h5>
            <p class="card-text">Team: {{team}}</p>
        </div>
    </div>
</div>